@model ICollection<GiftsSystem.Models.ApplicationUser>
@{
    ViewBag.Title = "SearchResult";
}

<h2>Results</h2>
@if (Model.Count == 0)
{
    <div>
        No results
    </div>
}
else
{
    foreach (var user in Model)
    {
        <div>
            @Html.ActionLink(user.UserName, "ShowDetails", "User", new { id = user.Id }, null)
        </div>
    }
}

