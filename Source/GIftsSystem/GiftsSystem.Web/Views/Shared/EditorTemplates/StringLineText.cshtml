@model string

<div class="form-group">
    @Html.LabelFor(model => model, new { @class = "col-lg-2 control-label" })
    <div class="col-lg-10">
        @Html.TextBoxFor(model => model, new { placeholder = "Name", @class = "form-control" })
    </div>
    @Html.ValidationMessageFor(m => m)
</div>
